// print the equalent binary value of given float value

#include <stdio.h>

int main()
{
    float num;
    printf("Enter the real number here:");
    scanf("%f",&num);
    int *ip=&num;
    printf("\nThe equalent binary of %f is : ",num);
    for(int i=31;i>=0;i--)
    printf("%d",(*ip)>>i&1);
    printf("\n");

    return 0;
}
